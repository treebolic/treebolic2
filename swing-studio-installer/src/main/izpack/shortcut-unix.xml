<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>

<izpack:shortcuts
	version="5.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:izpack="http://izpack.org/schema/shortcuts"
	xsi:schemaLocation="http://izpack.org/schema/shortcuts http://izpack.org/schema/5.0/izpack-shortcuts-5.0.xsd">

  <programGroup defaultName='Treebolic Studio' location='applications'/>

  <shortcut
		name='Treebolic Studio'
		programGroup='no'
		desktop='yes'
		applications='yes'
		startMenu='no'
		startup='no'
		target='java'
		commandLine='-jar "$INSTALL_PATH/swing-studio-@{appversion}-uber.jar"'
		workingDirectory='$INSTALL_PATH'
		description='Treebolic Studio'
		iconFile='$INSTALL_PATH/treebolic-studio.png'
		iconIndex='0'
		initialState='normal'
		encoding='UTF-8'
		type='Application'
		terminal='false' >
		<createForPack name='treebolic-studio'/>
  </shortcut>

</izpack:shortcuts>

